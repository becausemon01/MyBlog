{
    "extends": "@docusaurus/tsconfig",
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Docusaurus",
    "docs": "https://docusaurus.io/docs/typescript-support",
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "jsx": "react",
        "target": "ES2022",
        "lib": ["ES2022", "DOM"],
        "moduleResolution": "node",
        "module": "esnext",
        "noEmit": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@site/*": ["./*"]
        },
        "skipLibCheck": true
    },
    "exclude": [".docusaurus", "build"]
}
